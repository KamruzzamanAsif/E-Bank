<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.WithdrawController">
   <children>
      <Label layoutX="216.0" layoutY="27.0" text="Withdraw Money">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ListView fx:id="accountList" layoutX="28.0" layoutY="144.0" prefHeight="200.0" prefWidth="200.0" />
      <Label fx:id="accountLabel" layoutX="14.0" layoutY="108.0" text="Select and account to withdraw Money" />
      <Label layoutX="279.0" layoutY="170.0" text="Ener Amount" />
      <TextField fx:id="withdrawAmount" layoutX="385.0" layoutY="166.0" />
      <Button fx:id="withdrawButton" layoutX="431.0" layoutY="220.0" mnemonicParsing="false" onAction="#clickWithdraw" text="Withdraw" />
      <Label fx:id="withdrawSuccessful" layoutX="313.0" layoutY="310.0" prefHeight="16.0" prefWidth="229.0" />
      <Button fx:id="back" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#clickOnBackButton" text="Back" />
   </children>
</AnchorPane>
