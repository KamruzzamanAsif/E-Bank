      <TextField fx:id="accountType" layoutX="410.0" layoutY="176.0" />
      <Label layoutX="288.0" layoutY="130.0" prefHeight="16.0" prefWidth="106.0" text="Owner Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="288.0" layoutY="176.0" prefHeight="16.0" prefWidth="106.0" text="Account Type">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickOnBackButton" text="Back" />
